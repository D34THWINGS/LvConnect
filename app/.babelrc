{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": ["Chrome >= 60", "Safari >= 10.1", "iOS >= 10.3", "Firefox >= 54", "Edge >= 15"]
      },
      "modules": false,
      "useBuiltIns": "usage"
    }],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "presets": ["@babel/env", "@babel/react", "@babel/flow"]
    }
  }
}
